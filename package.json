{
  "name": "me",
  "version": "1.0.0",
  "scripts": {
    "dev": "run-s clean watch",
    "watch": "run-p watch:*",
    "watch:verify": "svelte-check --watch",
    "watch:build": "cross-env NODE_ENV=development rollup --config --watch",
    "clean": "rimraf dist",
    "verify": "svelte-check",
    "build": "run-s clean verify compile deploy",
    "compile": "cross-env NODE_ENV=production rollup --config",
    "deploy": "cdk deploy -c domain=siarheimelnik.dev -c subdomain=www"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "@rollup/plugin-typescript": "^8.2.0",
    "@tsconfig/svelte": "^1.0.10",
    "@types/node": "^14.14.25",
    "@xstate/fsm": "^1.6.0",
    "@xstate/svelte": "^0.1.0",
    "aws-cdk": "^1.89.0",
    "cross-env": "^7.0.3",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.39.0",
    "rollup-plugin-html2": "^2.0.0",
    "rollup-plugin-import-css": "^2.0.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-serve": "^1.1.0",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.32.7",
    "svelte": "^3.32.3",
    "svelte-check": "^1.1.34",
    "svelte-preprocess": "^4.6.9",
    "ts-node": "^9.1.1",
    "tslib": "^2.1.0",
    "typescript": "^4.1.5"
  },
  "dependencies": {
    "@aws-cdk/aws-certificatemanager": "^1.89.0",
    "@aws-cdk/aws-route53": "^1.89.0",
    "@aws-cdk/aws-route53-targets": "^1.89.0",
    "@aws-cdk/aws-s3": "^1.89.0",
    "@aws-cdk/aws-s3-deployment": "^1.89.0",
    "@aws-cdk/core": "^1.89.0"
  }
}
