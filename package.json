{
  "name": "me",
  "version": "1.0.0",
  "scripts": {
    "dev": "run-s clean watch",
    "watch": "run-p watch:*",
    "watch:verify": "svelte-check --watch",
    "watch:build": "rollup --config rollup.config.dom.js --watch",
    "clean": "rimraf static edge",
    "verify": "svelte-check && tsc --noEmit",
    "format": "prettier --write ./src/**/*.{svelte,html,ts}",
    "lint": "eslint --fix './src/**/*.{ts,svelte}'",
    "build": "run-s clean verify format lint compile edge",
    "compile": "rollup --config rollup.config.dom.js",
    "edge": "rollup --config rollup.config.edge.js",
    "deploy": "cdk deploy -c domain=siarheimelnik.dev -c subdomain=www"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^19.0.1",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@rollup/plugin-replace": "^2.4.1",
    "@rollup/plugin-typescript": "^8.2.3",
    "@tsconfig/svelte": "^1.0.10",
    "@types/node": "^15.14.3",
    "@types/page": "^1.11.3",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "aws-cdk": "^1.174.0",
    "eslint": "^7.31.0",
    "eslint-plugin-svelte3": "^3.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.2",
    "prettier-plugin-svelte": "^2.3.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.54.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-html2": "^2.0.0",
    "rollup-plugin-import-css": "^2.0.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-serve": "^1.1.0",
    "rollup-plugin-string": "^3.0.0",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.36.0",
    "svelte-check": "^2.2.3",
    "svelte-preprocess": "^4.7.4",
    "ts-node": "^10.1.0",
    "tslib": "^2.3.0",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "@aws-cdk/aws-certificatemanager": "^1.115.0",
    "@aws-cdk/aws-lambda": "^1.115.0",
    "@aws-cdk/aws-route53": "^1.115.0",
    "@aws-cdk/aws-route53-targets": "^1.115.0",
    "@aws-cdk/aws-s3": "^1.115.0",
    "@aws-cdk/aws-s3-deployment": "^1.115.0",
    "@aws-cdk/core": "^1.115.0",
    "@xstate/fsm": "^1.6.0",
    "@xstate/svelte": "^0.1.0",
    "page": "^1.11.6",
    "svelte": "^3.49.0"
  }
}
