{
  "name": "me",
  "version": "1.0.0",
  "scripts": {
    "dev": "run-s clean watch",
    "watch": "run-p watch:*",
    "watch:verify": "svelte-check --watch",
    "watch:build": "rollup --config rollup.config.dom.js --watch",
    "clean": "rimraf static edge",
    "verify": "svelte-check && tsc --noEmit",
    "build": "run-s clean verify compile edge",
    "compile": "rollup --config rollup.config.dom.js",
    "edge": "rollup --config rollup.config.edge.js",
    "deploy": "cdk deploy -c domain=siarheimelnik.dev -c subdomain=www"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "@rollup/plugin-replace": "^2.4.1",
    "@rollup/plugin-typescript": "^8.2.0",
    "@tsconfig/svelte": "^1.0.10",
    "@types/node": "^14.14.31",
    "@types/page": "^1.11.1",
    "aws-cdk": "^1.91.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "prettier-plugin-svelte": "^2.1.6",
    "rimraf": "^3.0.2",
    "rollup": "^2.40.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-html2": "^2.0.0",
    "rollup-plugin-import-css": "^2.0.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-serve": "^1.1.0",
    "rollup-plugin-string": "^3.0.0",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.32.8",
    "svelte-check": "^1.1.36",
    "svelte-preprocess": "^4.6.9",
    "ts-node": "^9.1.1",
    "tslib": "^2.1.0",
    "typescript": "^4.2.2"
  },
  "dependencies": {
    "@aws-cdk/aws-certificatemanager": "^1.91.0",
    "@aws-cdk/aws-lambda": "^1.91.0",
    "@aws-cdk/aws-route53": "^1.91.0",
    "@aws-cdk/aws-route53-targets": "^1.91.0",
    "@aws-cdk/aws-s3": "^1.91.0",
    "@aws-cdk/aws-s3-deployment": "^1.91.0",
    "@aws-cdk/core": "^1.91.0",
    "@xstate/fsm": "^1.6.0",
    "@xstate/svelte": "^0.1.0",
    "page": "^1.11.6",
    "svelte": "^3.34.0"
  }
